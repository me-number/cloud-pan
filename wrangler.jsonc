  // 配置 KV 和 D1 绑定
  "kv_namespaces": [{
    "binding": "cloud-pan",
    "id": "9e3dfaeda1ce43b284938b4319fa0629"  // 替换为你的 KV ID
  }],
  "d1_databases": [{
    "binding": "cloud-pan",
    "database_name": "cloud-pan",
    "database_id": "536aa98e-3390-4445-b159-fd5f0f0d20c5"  // 替换为你的 D1 ID
  }]
}